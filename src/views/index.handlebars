<div>
    <h1>Productos</h1>
    {{#if products}}
        {{#each products}}
            <div>
                <p>{{this.id}}</p>
                <p>{{this.title}}</p>
                <p>{{this.price}}</p>
            </div>
        {{/each}}
    {{else}}
        <h2>No se encontraron productos</h2>
    {{/if}}
</div>

<script src="/socket.io/socket.io.js"></script>
<script src="/js/index.js"></script>